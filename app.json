{
	"expo": {
		"owner": "bartosz-dude",
		"name": "Archive3 Reader",
		"slug": "archive3-reader",
		"version": "0.4.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "archive3-reader",
		"userInterfaceStyle": "automatic",
		"jsEngine": "hermes",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#BD0E0E"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#BD0E0E"
			},
			"package": "com.bartoszdude.archive3reader"
		},
		"androidStatusBar": {
			"translucent": true,
			"backgroundColor": "#00000000"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "d6efdf62-e429-4a84-bb54-c61f1582cd5d"
			}
		}
	}
}
